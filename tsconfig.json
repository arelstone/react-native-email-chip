{
    "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "jsx": "react-native"
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "**/*.spec.*",
        "**/*.test.*"
    ]
}
